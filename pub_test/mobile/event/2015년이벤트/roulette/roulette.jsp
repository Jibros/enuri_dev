function randomize($min, $max){
    return Math.floor(Math.random() * ($max - $min + 1)) + $min;
}
    
function layerPop(){
	$(".dimmed").height($(window).height());
	$('.layerPop').show();
	var layerPopHeight = $('.layerPop').find('.layerPopInner').height();
	$('.layerPop').find('.layerPopInner').css('margin-top','-'+ layerPopHeight / 2 +'px' );
	$('.layerPop').append('<div class="dimmed"></div>');
	$('html, body').addClass('dimdOn');
	
	$('.layerPop .btnClose').click(function(){
		var _this = $(this);
		$('.layerPop .dimmed').remove();
		$('html, body').removeClass('dimdOn');
		_this.parent().parent().parent().hide();
	});
	$('#bt_close').click(function(){
		var _this = $(this);
		$('.layerPop .dimmed').remove();
		$('html, body').removeClass('dimdOn');
		_this.parent().parent().parent().hide();
	});
		
		
	$("#bt_send").click(function(){
		send_btt();
	});
	
	$("#phoneno").focus(function(){
		$(this).css("color","#000000");
		if($(this).val() == '(-)없이 기입'){
			$(this).val('');
		}
	}).blur(function(){
		if($(this).val() == ''){
			$(this).css("color","#cccccc");
			$(this).val('(-)없이 기입');
			$(this).val() = $(this).val();
		}else{
			$(this).css("color","#000000");
		}
	});
	
	$(document).on("keyup", "input:text[numberOnly]", function() {$(this).val( $(this).val().replace(/[^0-9]/gi,"") );});
}
	
function layerPop_resize(){
	var layerPopHeight = $('.layerPop').find('.layerPopInner').height();
	$('.layerPop').find('.layerPopInner').css('margin-top','-'+ layerPopHeight / 2 + 'px' );
}
	
function send_btt(){

	if( $("#phoneno").val() ==  "" || $("#phoneno").val() == "(-)없이 기입"){
		$("#layerPopWarning").text("휴대폰 번호를 입력해 주세요.");
		return false;
	}
	if($("input:checkbox[id='agreechk']").is(":checked") == false){
		$("#layerPopWarning").text("개인 정보 수집에 동의해 주세요.");
		return false;
	}
	
	var vData = "phoneno="+$("#phoneno").val();

	$.ajax({
		type: "GET",
		url: "/mobilenew/event/set_roulette.jsp",
		data: vData,
		async: false,
		dataType:"JSON",
		success: function(json){
			alert(json.returnMsg);
			$('.layerPop .btnClose').click();
		},
		error: function (xhr, ajaxOptions, thrownError) {
			alert(xhr.status);
			alert(thrownError);
		}
	});
}
	
function chkNumber(){
	if((event.keyCode<48)||(event.keyCode>57))
		event.returnValue=false;
}

document.body.addEventListener('touchmove', function(e){
	if($('body').hasClass('dimdOn')){e.preventDefault();}
}, false);
