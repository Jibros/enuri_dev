/*딤레이어 공통 */
.pop-layer  {position:fixed; _position:absolute; top:0; left:0; width:100%; height:100%; z-index:1000;}
.popupLayer {position:absolute; left:0; top:0; width:100%; height:100%; z-index:100; -ms-filter: alpha(opacity=60); filter: alpha(opacity=60); opacity:0.6; -moz-opacity:0.6;}
.popupLayer .dim {position:absolute; left:0; top:0; width:100%; height:100%; background:#000; z-index:101;}
.popup_box {position:absolute; left:50%; top:50%; overflow:hidden; z-index:102;}
.popup_box h2, .popup_box .txt, .popup_box .btn em {overflow:hidden;position:absolute;left:-1000%;top:0;width:0;height:0;font-size:0;line-height:0;}
.popup_box img {vertical-align:top; margin:0; padding:0;}
.popup_box .pop-conts {position:absolute; left:0; top:0; width:100%; height:100%; z-index:102;}

