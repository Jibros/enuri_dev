@charset "utf-8";

@import url("default.css");


/* 20150608 구매인증 프로모션 */
#divScreen {display:none;position:fixed; top:0; left:0;width:0%; height:0%; background: url(http://imgenuri.enuri.gscdn.com/images/event/2015/prize/layer_back.png); z-index:10000;}
#divScreen.on{width:100%; height:100%;display:block;}

.event_wrap {position:relative; width:10; }
.event_wrap h2, .event_wrap h3, .event_wrap .txt {overflow:hidden;position:absolute;left:-1000%;top:0;width:0;height:0;font-size:0;line-height:0;}
.event_wrap a {outline:none;}
.section {overflow:hidden; }
.contents {position:relative; width:1056px; margin:0 auto;}

.visual {height:565px; background:url(http://imgenuri.enuri.gscdn.com/images/event/2015/certify/bg_visual.jpg) #040000 no-repeat 50% 0;}
.visual .contents {height:100%; background:url(http://imgenuri.enuri.gscdn.com/images/event/2015/certify/img_visual.png) no-repeat 50% 0;}

.inform {height:366px; background:url(http://imgenuri.enuri.gscdn.com/images/event/2015/certify/bg_inform.jpg) repeat-x 50% 0;}
.inform .contents {height:323px; background:url(http://imgenuri.enuri.gscdn.com/images/event/2015/certify/img_inform.png) no-repeat 50% 0;}
.inform .contents  a.btn_wiinner {position:absolute; bottom:0; right:51px; width:183px; height:34px; display:inline-block; text-indent:-9999em; }

.method {height:1501px; background-color:#eff8fd;}
.method .contents {height:1417px; margin-top:84px; background:url(http://imgenuri.enuri.gscdn.com/images/event/2015/certify/img_method.png) no-repeat 50% 0;}
.method .contents  a.btn_apply {position:absolute; bottom:123px; left:53px; width:949px; height:120px; display:inline-block; text-indent:-9999em; }

.slide {height:699px; background:url(http://imgenuri.enuri.gscdn.com/images/event/2015/certify/bg_slide.jpg) repeat-x 50% 0;}
.slide_box {position:relative; }
.pro_box {width:951px; margin:32px auto 0; }
.pro_list {overflow:hidden; margin-left:-60px;}
.pro_list li {position:relative; float:left; width:277px; height:416px; margin-left:60px;}
.pro_list li .link {display:block; width:100%; height:313px;}
.pro_list li .link img {width:277px; height:313px;}
.pro_list li .pro_name {display:block; overflow:hidden; height:20px; margin:18px 0 20px; font-family:malgun gothic; font-size:20px; color:#fff; line-height:20px; letter-spacing:-0.05em; text-align:center}
.pro_list li .pro_price {width:142px; height:20px; overflow:hidden; font-family:malgun gothic; font-size:15px; color:#fff; letter-spacing:-0.05em; }
.pro_list li .pro_price dt {float:left; width:44px; font-weight:bold;}
.pro_list li .pro_price dd {float:right; width:88px; text-align:right; font-weight:bold;}
.pro_list li .pro_price dd em {font-family:Tahoma;}
.pro_list li .pro_price.value {margin-bottom:5px; }
.pro_list li .pro_price.value dd {text-decoration:line-through;}
.pro_list li .btn_buying {position:absolute; right:1px; bottom:0; display:inline-block; width:114px; height:45px; text-indent:-9999em; background-image:url(http://imgenuri.enuri.gscdn.com/images/event/2015/certify/btn_buying.png);}

.slide_box a.navBtn {position:absolute;top:121px;width:28px;height:59px;display:block;text-indent:-100%;*text-indent:0;overflow:hidden;font-size:0;line-height:0; z-index:10;}
.slide_box a.navBtn:hover {opacity:.4; -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=40)"; filter: progid:DXImageTransform.Microsoft.Alpha(Opacity=40);}
.slide_box a.btn_prev {left:0; background:url(http://imgenuri.enuri.gscdn.com/images/event/2015/certify/btn_slide_prv.png) 0 0 no-repeat;}
.slide_box a.btn_next {right:0; background:url(http://imgenuri.enuri.gscdn.com/images/event/2015/certify/btn_slide_next.png) 0 0 no-repeat;}

.experiences {height:2919px; background-color:#ededed;}
.experiences .contents {height:2698px; margin-top:114px; background:url(http://imgenuri.enuri.gscdn.com/images/event/2015/certify/img_experiences.png) no-repeat 50% 0;}




/*팝업 */
.popup {position:relative; margin:0 auto; overflow:hidden; }
.popup h2, .popup .txt {overflow:hidden;position:absolute;left:-1000%;top:0;width:0;height:0;font-size:0;line-height:0;}
.popup a {outline:none;}
.popup img {vertical-align:top; margin:0; padding:0;}

/* 정보 동의 공통 */
.popup .guide {width:413px; height:140px; padding:20px 15px; line-height:17px; color:#999; overflow-y:auto;line-height:18px; background-color:#fff;}
.popup .guide .tit {font-weight:bold; margin:20px 0;}
.popup .guide .tit.mgt0 {margin-top:0;}
.popup .guide ol, .popup .guide ul { margin-top:10px; padding-bottom:9px;}
.popup .guide ol {margin-left:16px;}
.popup .guide ol li span {margin-left:-14px;}
.popup .guide ul {margin-left:0;}
.popup .agree {width:196px; }
.popup .agree * {vertical-align:middle;}
.popup .agree input[type="checkbox"] {margin-top:-1px; margin-right:5px; background-color:none;}
.popup .agree label {font-family:"돋움", dotum; font-size:12px;  color:#767676; font-weight:bold;}

.pop1 {width:980px; height:805px; overflow-y:hidden;} 
.pop1:after {content:""; display:block; clear:both;}
.pop1 .f_left {float:left; width:480px; height:797px; overflow-x:hidden; overflow-y:auto; border:4px solid #055695; border-right:none;}
.pop1 .f_right {position:relative; float:left; width:479px; height:805px; background:url(http://imgenuri.enuri.gscdn.com/images/event/2015/certify/pop1_bg.jpg) no-repeat 0 0;}
.pop1 .f_right input[type="text"] {height:26px; font-family:"돋움", dotum; font-size:12px; color:#767676; line-height:26px; padding:0 10px; border:1px solid #c6c9cd; background-color:#fff;}
.pop1 .f_right .nickname {position:absolute; left:151px;  top:84px;}
.pop1 .f_right .nickname input {width:248px;}
.pop1 .f_right .phonenumber {position:absolute; left:151px;  top:124px;}
.pop1 .f_right .phonenumber input {width:58px; margin-right:18px;} 
.pop1 .f_right .phonenumber input.first {width:44px;}
.pop1 .f_right .buymall {position:absolute; left:151px;  top:164px;}
.pop1 .f_right .buymall .select_box {width: 178px; }
.pop1 .f_right .buydate {position:absolute; left:151px;  top:204px; overflow:hidden;}
.pop1 .f_right .buydate .select_box {float:left; }
.pop1 .f_right .buydate .select_box.sb1 {width:78px; margin-right:10px;}
.pop1 .f_right .buydate .select_box.sb2 {width:88px; }
.select_box {position: relative;height: 28px; border: 1px solid #b5b5b5;background: url(http://imgenuri.enuri.gscdn.com/images/event/2015/mass_marketing2/bul_select_arrow.gif) #fff 100% 50% no-repeat;}
.select_box label {position: absolute;  top: 8px; left: 10px; font-family:"돋움", dotum; font-size:12px; 	color: #767676;}
.select_box select.selectbar {width:100%; height: 28px; min-height: 28px;	line-height:28px; padding: 0 10px;	opacity: 0;	filter: alpha(opacity=0);	}
.select_box .select_box_sel {width:100%; height: 28px; min-height: 28px;	line-height:28px; padding: 0 10px;	opacity: 0;	filter: alpha(opacity=0);	}
.pop1 .f_right .ordernumber {position:absolute; left:151px;  top:244px;}
.pop1 .f_right .ordernumber input {width:248px;}
.pop1 .f_right .guide {position:absolute; top:304px; left:17px; }
.pop1 .f_right .agree {position:absolute; top:494px; left:129px;}
.pop1 .f_right a.submit {position:absolute;  top:534px; left:108px; display:inline-block; width:262px; height:42px; text-indent:-9999em;  background:url(/images);}
.pop1 .f_right a.close {position:absolute; top:16px; right:19px; width:18px; height:18px; text-indent:-9999em;  background:url(/images); }

.pop2 {width:600px; height:529px; background-image:url(http://imgenuri.enuri.gscdn.com/images/event/2015/certify/bg_pop2.jpg); }
.pop2 .winDate {position:absolute; top:156px; left:168px; width:262px; text-align:center; line-height:16px; font-family:"돋움", dotum; font-weight:bold; color:#fff; font-size:16px; }
.pop2 .sel_date {position:absolute; top:166px; right:40px;}
.pop2 .sel_date select {width:90px; color:#535353; border:1px solid #cececc; }
.pop2 .prize {overflow:hidden; position:absolute; top:284px; left:132px;}
.pop2 .prize ul li {float:left; width:160px; color:#535353; height:24px; line-height:24px;  font-family:"돋움", dotum;  font-size:12px;}
.pop2 .prize ul li {margin-right:28px; }
.pop2 .prize ul li * {vertical-align:middle;}
.pop2 .prize ul li .nick {display:inline-block; width:76px; overflow:hidden;white-space:nowrap; }

.pop3 {width:500px; height:428px;  background-image:url(http://imgenuri.enuri.gscdn.com/images/event/2015/certify/bg_pop3.jpg);}
.pop3 .order {padding-top:89px; width:420px; margin:0 auto; text-align:center;}
.pop3 .order p {margin-top:3px; font-family:malgun gothic; font-weight:600; font-size:27px; line-height:27px; height:30px; overflow:hidden; letter-spacing:-0.05em; color:#055695;}

.pop3 a.btn_submit {position:absolute;  bottom:29px; left:146px; display:inline-block; width:209px; height:42px; text-indent:-9999em;}




/* 퀵메뉴 */
#quick_menu {position:absolute; left:50%; top:0px; width:86px; height:332px; margin-left:520px; z-index: 600;}
#quick_menu li a {display:block; width:86px; height:83px; background: url(http://imgenuri.enuri.gscdn.com/images/event/2015/mass_marketing2/quick_menu.png) no-repeat 0 0;}
#quick_menu li:first-child a {margin-top:0;}
#quick_menu li a.top {background-position:0 0;}
#quick_menu li a.event1 {background-position:0 -83px ;}
#quick_menu li a.event2 {background-position:0 -166px;}
#quick_menu li a.event3 {background-position:0 -249px;}
#quick_menu li a  > span {overflow:hidden;position:absolute;left:-1000%;top:0;width:0;height:0;font-size:0;line-height:0;}





/* ------------------------ 개발 ------------------------  */
.pagingMain { margin:0 auto;height:40px;font-size:24px;}
.pagingMain span {
    padding-top:5px;
    padding-right:2px;
    padding-left:2px;
    width:17px;
    height:17px;
    border:1px solid #D6E3E9;
    font-family: Arial;
    margin-right:5px;
    text-align:center;
    cursor:pointer;
    font-size:12px;
    color:#006699;
    text-decoration:none;
    display:inline-block
}
.pagingMain span:hover {
    border:1px solid #ff0000;
    color:#FF6600;
}

.pagingMain span.on {
    text-decoration:none;
    color:#FF6600;
    border:2px solid #ff0000;
    color:#FF6600;
}


/* display 관련. 2015.05.20 추가. */
.displayblock {display:block;visibility:visible;}
.displaynone {display:none;visibility:hidden;}

/*150520 푸터*/
.bnr_area{margin-top:8px;background:url(http://imgenuri.enuri.gscdn.com/images/main/bg_footer_bnr.gif) 0 0; text-align:center; height:91px; }
.bnr_area ul{margin:0 auto; width:1000px; background:url(http://imgenuri.enuri.gscdn.com/images/main/footer_bnr.gif) 0 0 no-repeat; height:91px;}
.bnr_area ul li a{width:50%; float:left; display:block; height:91px; text-indent:-9999em; }
.footer{width:1000px; font-family:dotum; font-size:12px; margin:0 auto; padding:0; font-family: 맑은 고딕,Malgun Gothic,Dotum,applegothic,sans-serif,arial;}
.footer_menu{text-align:center; height: 28px; padding:13px 0 0 0;  padding-top: 10px; border-bottom:1px solid #e2e2e2; background:#f2f2f2; margin:0;}
.footer_menu li{margin:auto; display:inline; margin:0 17px;}
.footer_menu li a{color:#555555; text-decoration:none;}

.footer .adr{padding: 14px 0 12px 23px;  font-size:12px;  color:#808080; line-height:16px; margin:0;}
.footer .adr span{padding-right:12px; }
.footer .adr img{vertical-align:middle}

.footer .award_list{background:url(http://imgenuri.enuri.gscdn.com/images/main/footer_bnr02.gif) 0 0 no-repeat; height:51px; text-indent:-9999em; margin:0; padding:0;}
.footer .copyright{padding: 10px 0 20px 23px; line-height:16px;  font-size:11px;  color:#808080; }
.footer .copyright strong{display:block; font-weight:normal; color:#5390be; }

#m_footer .m_footInner address {float:left;margin:0 4px 0 0;color:#888;font-style:normal;}

