			return false;
		}
		isClick = false;
		var result=0; //대상자 체크
		$.ajax({
			type: "POST",
			url: "/mobilefirst/evt/ajax/ajax_welcomechk_v2.jsp?ostpcd="+getOsType(),
			dataType: "JSON",
